{
  "name": "@loopback/express-middleware",
  "version": "0.0.1",
  "description": "LoopBack extension for Express middleware",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "acceptance": "lb-mocha \"dist/__tests__/acceptance/**/*.js\"",
    "build": "lb-tsc",
    "clean": "lb-clean loopback-express-middleware*.tgz dist tsconfig.build.tsbuildinfo package",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "unit": "lb-mocha \"dist/__tests__/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-express-middleware*.tgz && tree package && npm run clean"
  },
  "author": "IBM Corp.",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@loopback/context": "^2.0.0",
    "@types/express": "^4.17.2",
    "@types/express-serve-static-core": "^4.17.2",
    "debug": "^4.1.1",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@loopback/build": "^3.1.0",
    "@loopback/eslint-config": "^5.0.2",
    "@loopback/testlab": "^1.10.2",
    "@types/cors": "^2.8.6",
    "@types/debug": "^4.1.5",
    "@types/node": "^10.17.14",
    "cors": "^2.8.5"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git",
    "directory": "packages/express-middleware"
  }
}
