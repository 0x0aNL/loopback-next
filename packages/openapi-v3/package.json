{
  "name": "@loopback/openapi-v3",
  "version": "0.5.5",
  "description": "Processes openapi v3 related metadata",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@loopback/build": "^0.4.3",
    "@loopback/openapi-spec-builder": "^0.4.5",
    "@loopback/repository": "^0.6.1",
    "@loopback/testlab": "^0.6.0",
    "@types/debug": "0.0.30",
    "@types/lodash": "^4.14.106",
    "@types/node": "^8.10.4"
  },
  "scripts": {
    "build": "lb-tsc es2017",
    "build:apidocs": "lb-apidocs",
    "clean": "lb-clean loopback-openapi-v3*.tgz dist* package",
    "integration": "lb-mocha \"DIST/test/integration/**/*.js\"",
    "pretest": "npm run build",
    "test": "lb-mocha \"DIST/test/unit/**/*.js\" \"DIST/test/integration/**/*.js\"",
    "unit": "lb-mocha \"DIST/test/unit/**/*.js\"",
    "verify": "npm pack && tar xf loopback-openapi-v3*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "keywords": [
    "Swagger",
    "OpenAPI Spec",
    "TypeScript"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist/src",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  },
  "dependencies": {
    "@loopback/context": "^0.6.1",
    "@loopback/openapi-v3-types": "^0.3.5",
    "@loopback/repository-json-schema": "^0.4.5",
    "debug": "^3.1.0",
    "lodash": "^4.17.5"
  }
}
